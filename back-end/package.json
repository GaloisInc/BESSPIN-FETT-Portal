    "eslint-config-fivetalent": "0.0.19",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.0",
